package model.vo.member;

public class Member {
	private String id;
	private String password;
	private String name;
	private int accountPlan;
	private int theme;
	
	
	public Member() {
		super();
	}

	// #00007 : (sychoi) : 생성자는 추후가 될 예정
	public Member(String id, String password, String name, int accountPlan, int theme) {
		super();
		this.id = id;
		this.password = password;
		this.name = name;
		this.accountPlan = accountPlan;
		this.theme = theme;
	}
	
	

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAccountPlan() {
		return accountPlan;
	}

	public void setAccountPlan(int accountPlan) {
		this.accountPlan = accountPlan;
	}

	public int getTheme() {
		return theme;
	}

	public void setTheme(int theme) {
		this.theme = theme;
	}

	@Override
	public String toString() {
		return "Member [id=" + id + ", password=" + password + ", name=" + name + ", accountPlan=" + accountPlan
				+ ", theme=" + theme + "]";
	}
	
	
}
