package string.step1.test;

public class StringExamTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "ABC";
		System.out.print("1. Length of str : " + str.length() + "\n");
		
		str.replace('C', 'D');
		System.out.print("2. '3rd char' --> 'D' : " + str + "\n");
		
	}

}
