	<script type="text/javascript">
		$(function() {
			// 1. Tag : span	
			$('span').css('color','green').css('border','2px solid orange');
		
			// 2. class : my
			$('.my').css('border', '5px dotted purple');
			
			// 3. id : content1
			$('#content1').css('background', 'yellow');
		})
	</script>

