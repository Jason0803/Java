package props.test;

import java.util.HashMap;

public class HashMapTest {
// ** Wrapper Class ** //
/*
 * Character, Short, Integer, ...
 * Integer score = new Integer(80);
 *  
 */
	public static void main(String[] args) {
		HashMap<String, Integer> studentGrades = new HashMap<String, Integer>();
		studentGrades.put("IU", 80);
		studentGrades.put("Jason", 100);
		studentGrades.put("Sunny", 80);
		studentGrades.put("Kim", 65);
	}

}
