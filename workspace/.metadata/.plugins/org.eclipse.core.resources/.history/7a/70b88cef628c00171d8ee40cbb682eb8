package com.stack.service;

import java.util.Stack;

public class PostfixCalculator {
	
	public int precendent(char op) {
		Stack<Integer> equiv = new Stack<Integer>();
		
		switch(op) {
	        case '(' : case ')' : return 0;
	        case '+' : case '-' : return 1;
	        case '*' : case '/' : return 2;
		}
		return -1;
	}
	
}
