package pizza.parent;

public class Pizza {
	private int price;
	private String storeName;
	
	public Pizza() {
		super();
	}

	public Pizza(int price, String storeName) {
		super();
		this.price = price;
		this.storeName = storeName;
	}
	
	public void dough() {
		System.out.println("Make Dough");
	}
	
	public void bake() {
		System.out.println("Bake on 180 degree");
	}
	
	abstract void topping();
	
}
