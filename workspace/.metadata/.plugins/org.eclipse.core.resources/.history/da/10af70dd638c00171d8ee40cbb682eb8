package com.stack.service;

import java.util.Stack;

public class PostfixCalculator {
	Stack<Integer> equivStack = new Stack<Integer>();
	
	public int precendent(char op) {
		switch(op) {
	        case '(' : case ')' : return 0;
	        case '+' : case '-' : return 1;
	        case '*' : case '/' : return 2;
		}
		return -1;
	}
	
