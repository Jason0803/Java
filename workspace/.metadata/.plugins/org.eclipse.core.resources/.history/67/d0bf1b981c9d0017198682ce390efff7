<script type="text/javascript">
	function choiceAngel() {
		var result = ""; 
		var list = document.getElementsByClassName('checkbox');
		for(var i=0; list[i]; ++i){
		      if(list[i].checked){}
		      		result += inputElements[i].value+", ";
		      }
		}
		if(result === "") alert("NULL~");
		else alert("Your choice : "+result);
	    
		/*
		 1. 한명의 이상형을 선택하지 않았다면
		    "한 명 이상은 선택!!" 메세지 띄우고
		 2. 한명 이상을 선택했다면
		    "당신이 선택하신 이상형은 ~, ~, ~...입니다"를 띄운다
		*/
	}
</script>
