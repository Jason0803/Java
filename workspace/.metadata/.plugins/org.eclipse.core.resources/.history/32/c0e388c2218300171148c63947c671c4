package com.vo;

public class Student {
	private String name;
	private int age;
	private String address;
	private int[] attendances;
	
	public Student(String name, int age, String address, int[] attendances) {
		super();
		this.name = name;
		this.age = age;
		this.address = address;
		this.attendances = attendances;
	}
	
	
}
