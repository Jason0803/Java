<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <filter>
    <filter-name>EncodingFilter</filter-name>
    <filter-class>servlet.filter.EncodingFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>EncodingFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>RegisterMemberServlet</servlet-name>
    <servlet-class>context.servlet.RegisterMemberServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RegisterMemberServlet</servlet-name>
    <url-pattern>/RMS</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>FindMemberServlet</servlet-name>
    <servlet-class>context.servlet.FindMemberServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FindMemberServlet</servlet-name>
    <url-pattern>/FMS</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ShowAllMemberServlet</display-name>
    <servlet-name>ShowAllMemberServlet</servlet-name>
    <servlet-class>context.servlet.ShowAllMemberServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ShowAllMemberServlet</servlet-name>
    <url-pattern>/ShowAllMember</url-pattern>
  </servlet-mapping>
</web-app>