<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <listener>
    <listener-class>listener.DriverLoadingListener</listener-class>
  </listener>
  <context-param>
    <param-name>ODriver</param-name>
    <param-value>oracle.jdbc.driver.OracleDriver</param-value>
  </context-param>
  <servlet>
    <description></description>
    <display-name>EntranceServlet</display-name>
    <servlet-name>EntranceServlet</servlet-name>
    <servlet-class>servlet.context.EntranceServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EntranceServlet</servlet-name>
    <url-pattern>/EntranceServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ViewServlet</display-name>
    <servlet-name>ViewServlet</servlet-name>
    <servlet-class>servlet.context.ViewServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ViewServlet</servlet-name>
    <url-pattern>/ViewServlet</url-pattern>
  </servlet-mapping>
</web-app>